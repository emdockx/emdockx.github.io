# Set pagination: Garmin returns a maximum of 100 at a time
batch_size <- 10L
start <- 0L
# Extract Garmin data
repeat {
cat("Fetching activities from", start, "to", start + batch_size - 1, "\n")
batch <- client$get_activities(start, batch_size)
if (length(batch) == 0) break
all_activities <- c(all_activities, batch)
start <- start + batch_size
}
# Helper function to extract fields from each activity safely
extract_activity_data <- function(act) {
tibble(
id = act$activityId,
type = act$activityType$typeKey,
date = as.Date(act$startTimeLocal),
distance = act$distance,
time = act$duration,
time_elapsed = act$elapsedDuration,
time_moving = act$movingDuration,
elevation_gain = act$elevationGain,
elevation_loss = act$elevationLoss,
speed = act$averageSpeed * 3.6,
speed_max = act$maxSpeed * 3.6,
calories = act$calories,
calories_brm = act$bmrCalories,
hr = act$averageHR,
hr_max = act$maxHR,
cadence = act$averageBikingCadenceInRevPerMinute,
cadence_max = act$maxBikingCadenceInRevPerMinute,
aerobic_effect = act$aerobicTrainingEffect,
anaerobic_effect = act$anaerobicTrainingEffect,
strokes = act$strokes,
temperature_min = act$minTemperature,
temperature_max = act$maxTemperature,
elevation_min = act$minElevation,
elevation_max = act$maxElevation,
respiration_min = act$minRespirationRate,
respiration_max = act$maxRespirationRate,
respiration = act$avgRespirationRate
)
}
